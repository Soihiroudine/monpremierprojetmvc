<section>
    <table class="table table-hover table-striped table-bordered text-center">
        <caption>Membre TV</caption>
        <tr>
            <th>ID</th>
            <th>NOM</th>
            <th>PRENOM</th>
            <th>TRAVAIL</th>
        </tr>

        <% membre.forEach(element=> { %>
            <tr>
                <td>
                    <%= element.id %>
                </td>
                <td>
                    <%= element.nom %>
                </td>
                <td>
                    <%= element.prenom %>
                </td>
                <td>
                    <%= element.travail %>
                </td>
            </tr>
            <% }); %>
    </table>
</section>